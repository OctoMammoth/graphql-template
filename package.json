{
	"name": "graphql-template",
	"version": "0.1.0",
	"description": "GraphQL Server on NodeJS Template",
	"main": "index.js",
	"scripts": {
		"dev": "npx ts-node-dev ./src/app.ts --project tsconfig.json",
		"resetDB": "npx prisma migrate reset --force",
		"start": "npx ts-node ./src/app.ts --project tsconfig.json",
		"generate": "npx ts-node generate.ts",
		"migrate": "prisma migrate dev --name init"
	},
	"author": "OctoM",
	"license": "ISC",
	"dependencies": {
		"@graphql-tools/merge": "^8.3.1",
		"@graphql-tools/schema": "^8.5.1",
		"@paljs/plugins": "^4.1.0",
		"@prisma/client": "^3.11.1",
		"apollo-server-cache-redis": "^3.3.1",
		"apollo-server-core": "^3.10.0",
		"apollo-server-express": "^3.10.0",
		"bcryptjs": "^2.4.3",
		"esm": "^3.2.25",
		"express": "^4.18.1",
		"graphql": "^16.5.0",
		"graphql-middleware": "^6.1.32",
		"graphql-shield": "^7.5.0",
		"graphql-upload": "^15.0.2",
		"ioredis": "^5.2.2",
		"jsonwebtoken": "^8.5.1",
		"shortid": "^2.2.16"
	},
	"devDependencies": {
		"@paljs/generator": "^4.1.0",
		"@paljs/schema": "^4.1.0",
		"@types/node": "^18.6.3",
		"prisma": "^3.11.1",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.7.4"
	}
}
